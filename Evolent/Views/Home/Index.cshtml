@model IEnumerable<Evolent.Web.ViewModels.ContactViewModel>

@{
    ViewBag.Title = "Store";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<p>
</p>
<div class="container">

    <div class="jumbotron">
        <div class="panel-heading">
            @using (Html.BeginForm("Create", "Home", FormMethod.Post,
                    new { enctype = "multipart/form-data", @class = "form-inline" }))
            {

                <div class="form-group">
                    <label class="sr-only" for="showFirstName">First Name</label>
                    <input type="text" class="form-control" name="showFirstName" placeholder="First Name required">
                </div>
                <div class="form-group">
                    <label class="sr-only" for="showLastName">Last Name</label>
                    <input type="text" class="form-control" name="showLastName" placeholder="Last Name">
                </div>
                <div class="form-group">
                    <label class="sr-only" for="showEmail">Email</label>
                    <input type="email" class="form-control" name="showEmail" placeholder="required example@test.com">
                </div>
                <div class="form-group">
                    <label class="sr-only" for="showMobileNumber">Mobile Number</label>
                    <input type="text" class="form-control" name="showMobileNumber" placeholder="10 digit Mob Num required">
                </div>
                <button type="submit" class="btn btn-primary">Add</button>
            }
        </div>

        
        @foreach (var item in Model)
        {
            <div class="panel panel-default">
                <div class="panel-heading">
                </div>

                @Html.Partial("Contact", item)
            </div>
        }

    </div>
</div>
